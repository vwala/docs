{
  "name": "am-docs",
  "version": "1.2.4",
  "description": "vwa.la documentation",
  "repository": "git@github.com:vwala/vwala.github.io.git",
  "author": "vwa.la",
  "main": "app.js",
  "scripts": {
    "clean": "rm -rf .cache && rm -rf public && rimraf node_modules",
    "deploy": "gatsby build && gh-pages -d public -b gh-pages",
    "serve": "gatsby build && NODE_ENV=production gatsby serve",
    "build": "gatsby build",
    "dev": "gatsby develop",
    "lint": "eslint . --ext .js --cache",
    "posttest": "yarn lint",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test dev http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test dev http://localhost:8000 cy:run",
    "test": "NODE_ENV=testing mocha -- $(find test -name '*.test.js') && yarn test:e2e:ci",
    "preship": "yarn test",
    "ship": "STATUS=$(git status --porcelain); echo $STATUS; if [ -z \"$STATUS\" ]; then yarn version && git push --follow-tags; fi",
    "master": "git pull -r origin master --autostash && cd content/api/v2 && git pull -r origin master --autostash && cd ../../../ && yarn"
  },
  "devDependencies": {
    "algoliasearch": "3.32.0",
    "autoprefixer": "9.3.1",
    "cssnano": "4.1.7",
    "cypress": "3.1.2",
    "cypress-testing-library": "2.3.3",
    "date-fns": "^1.30.1",
    "eslint": "5.9.0",
    "eslint-plugin-ghost": "0.0.27",
    "eslint-plugin-react": "7.11.1",
    "gh-pages": "^2.0.1",
    "ghost-spirit": "0.0.49",
    "mocha": "5.2.0",
    "postcss-color-mod-function": "3.0.3",
    "postcss-css-variables": "0.11.0",
    "postcss-custom-media": "7.0.7",
    "postcss-custom-properties": "8.0.9",
    "postcss-easy-import": "3.0.0",
    "should": "13.2.3",
    "sinon": "7.0.0",
    "start-server-and-test": "1.7.11",
    "write-yaml": "^1.0.0"
  },
  "dependencies": {
    "@tryghost/helpers": "1.0.0",
    "@tryghost/helpers-gatsby": "1.0.0",
    "algolia-html-extractor": "0.0.1",
    "bluebird": "^3.5.3",
    "core-js": "^2.6.12",
    "gatsby": "^3.8.1",
    "gatsby-image": "^3.8.0",
    "gatsby-plugin-algolia": "^0.20.1",
    "gatsby-plugin-catch-links": "^3.8.0",
    "gatsby-plugin-force-trailing-slashes": "^1.0.5",
    "gatsby-plugin-google-analytics": "^3.8.0",
    "gatsby-plugin-intercom-spa": "^0.2.0",
    "gatsby-plugin-manifest": "^3.8.0",
    "gatsby-plugin-offline": "^4.8.0",
    "gatsby-plugin-postcss": "^4.8.0",
    "gatsby-plugin-react-helmet": "^4.8.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^3.8.0",
    "gatsby-plugin-sitemap": "^4.4.0",
    "gatsby-remark-autolink-headers": "^4.5.0",
    "gatsby-remark-code-titles": "^1.1.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^5.5.0",
    "gatsby-remark-prismjs": "^5.5.0",
    "gatsby-source-filesystem": "^3.8.0",
    "gatsby-transformer-remark": "^4.5.0",
    "gatsby-transformer-sharp": "^3.8.0",
    "gatsby-transformer-yaml": "^3.8.0",
    "install": "^0.13.0",
    "prismjs": "^1.24.1",
    "react": "^16.14.0",
    "react-autosuggest": "9.4.2",
    "react-dom": "^16.14.0",
    "react-helmet": "5.2.0",
    "react-instantsearch-dom": "5.3.2",
    "react-intercom": "^1.0.15",
