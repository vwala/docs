{
  "name": "am-docs",
  "version": "2.0.1",
  "private": true,
  "description": "vwa.la documentation",
  "repository": "git@github.com:vwala/vwala.github.io.git",
  "author": "vwa.la",
  "keywords": [
    "vwa.la",
    "documentation",
    "help"
  ],
  "scripts": {
    "setnode": "source $NVM_DIR/nvm.sh && nvm use 20",
    "clean": "gatsby clean && rm -rf lib node_modules",
    "cleannode": "rm -rf lib node_modules",
    "generate": "npm run setnode && tsc ./utils/content-generator.ts && node ./utils/content-generator.js",
    "develop": "cross-env NODE_ENV=development gatsby develop",
    "start": "npm run setnode && gatsby develop",
    "build": "npm run setnode && gatsby build",
    "serve": "npm run setnode && gatsby serve",
    "typecheck": "tsc --noEmit",
    "deploy": "cross-env NODE_ENV=production gatsby build && gh-pages -d public -b gh-pages"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mdx-js/react": "^2.3.0",
    "@mui/icons-material": "^5.15.14",
    "dotenv": "^16.4.5",
    "gatsby": "^5.13.3",
    "gatsby-omni-font-loader": "^2.0.2",
    "gatsby-plugin-algolia": "^1.0.3",
    "gatsby-plugin-emotion": "^8.13.1",
    "gatsby-plugin-google-gtag": "^5.13.1",
    "gatsby-plugin-image": "^3.13.1",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-mdx": "^5.13.1",
    "gatsby-plugin-react-svg": "^3.3.0",
    "gatsby-plugin-sharp": "^5.13.1",
    "gatsby-plugin-sitemap": "^6.13.1",
    "gatsby-remark-images": "^7.13.1",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-transformer-sharp": "^5.13.1",
    "react": "^18.2.0",
    "react-autosuggest": "^10.1.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-instantsearch-dom": "^6.40.4",
    "react-modal": "^3.16.1",
    "rehype-autolink-headings": "^4.0.0",
    "rehype-slug": "^4.0.1",
    "remark-gfm": "^1.0.0",
    "tocbot": "^4.25.0",
    "url": "^0.11.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.70",
    "@types/react-dom": "^18.2.22",
    "algolia-html-extractor": "^0.0.1",
    "algoliasearch": "^4.22.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "fs": "^0.0.1-security",
    "gh-pages": "^5.0.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "request": "^2.88.2",
    "typescript": "^5.4.3",
    "util": "^0.12.5",
    "write-yaml": "^1.0.0"
  }
}